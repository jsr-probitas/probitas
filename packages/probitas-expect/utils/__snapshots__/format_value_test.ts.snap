export const snapshot = {};

snapshot[`formatValue > formats null 1`] = `"null"`;

snapshot[`formatValue > formats undefined 1`] = `"undefined"`;

snapshot[`formatValue > formats string 1`] = `"hello world"`;

snapshot[`formatValue > formats number 1`] = `"42"`;

snapshot[`formatValue > formats boolean 1`] = `"true"`;

snapshot[`formatValue > formats simple object 1`] = `
'{
  age: 30,
  name: "Alice",
}'
`;

snapshot[`formatValue > formats nested object 1`] = `
'{
  active: true,
  user: {
    address: {
      city: "Tokyo",
      country: "Japan",
    },
    age: 30,
    name: "Alice",
  },
}'
`;

snapshot[`formatValue > formats array 1`] = `
"[
  1,
  2,
  3,
  4,
  5,
]"
`;

snapshot[`formatValue > formats array of objects 1`] = `
'[
  {
    id: 1,
    name: "Alice",
  },
  {
    id: 2,
    name: "Bob",
  },
  {
    id: 3,
    name: "Charlie",
  },
]'
`;
